{
  "name": "ui-process-api",
  "version": "1.0.0",
  "description": "API server for UI Process application with PostgreSQL and pgvector support",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "setup": "npm run db:setup && npm run db:seed",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "node scripts/run-migrations.js",
    "db:seed": "node scripts/seed-database.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^4.0.1",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "workflow",
    "process-management",
    "postgresql",
    "pgvector",
    "api",
    "vector-search",
    "rag"
  ],
  "author": "UI Process Development Team",
  "license": "MIT"
}
